{
  "name": "cc-web-term",
  "version": "1.0.0",
  "description": "A ComputerCraft terminal for the internet",
  "repository": {
    "type": "git",
    "url": "https://github.com/SquidDev-CC/cc-web-term.git"
  },
  "keywords": [
    "computercraft"
  ],
  "author": "SquidDev <squid@squiddev.cc>",
  "license": "BSD-3-Clause",
  "scripts": {
    "prepare:setup": "node tools/setup.js",
    "prepare:tsc": "tsc --project .",
    "prepare": "npm run prepare:setup && npm run prepare:tsc",
    "lint": "tslint --project .",
    "example:tsc": "tsc --project example",
    "example:rollup": "cd example && rollup -c rollup.config.js",
    "example:host": "python -m http.server 8080"
  },
  "dependencies": {
    "gif.js": "^0.2.0",
    "preact": "^10.2.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-url": "^4.0.1",
    "postcss": "^7.0.26",
    "postcss-selector-parser": "^6.0.2",
    "rollup": "^1.29.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-typescript2": "^0.25.3",
    "rollup-plugin-webworkify": "0.0.4",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4"
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "assets",
    "dist"
  ]
}
