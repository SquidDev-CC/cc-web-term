{
  "name": "cc-web-term",
  "version": "1.0.0",
  "description": "A ComputerCraft terminal for the internet",
  "repository": {
    "type": "git",
    "url": "https://github.com/SquidDev-CC/cc-web-term.git"
  },
  "keywords": [
    "computercraft"
  ],
  "author": "SquidDev <squid@squiddev.cc>",
  "license": "BSD-3-Clause",
  "scripts": {
    "prepare:setup": "node tools/setup.js",
    "prepare:tsc": "tsc --project .",
    "prepare": "npm run prepare:setup && npm run prepare:tsc",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "example:tsc": "tsc --project example",
    "example:rollup": "cd example && rollup -c rollup.config.js",
    "example:host": "python -m http.server 8080"
  },
  "dependencies": {
    "gif.js": "^0.2.0",
    "preact": "^10.3.4"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-url": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "postcss": "^7.0.27",
    "postcss-selector-parser": "^6.0.2",
    "rollup": "^1.32.1",
    "rollup-plugin-postcss": "^2.4.1",
    "typescript": "^3.8.3"
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "assets",
    "dist"
  ]
}
